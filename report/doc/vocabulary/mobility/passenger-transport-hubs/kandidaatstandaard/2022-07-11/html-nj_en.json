    },
    {
      "uri": "https://purl.eu/ns/mobility/passenger-transport-hubs#Hoppinpoint",
      "name": "Hoppinpoint",
      "label": {
        "en": "Hoppinpoint"
      },
      "sort": "Hoppinpoint",
      "description": {
        "en": "Transport hub of the mobility brand Hoppin."
      },
      "usage": {
        "en": "Hoppin is the name under which the Flemish government indicates transport hubs for passenger transport that meet fixed (quality) requirements. Important aspects are recognisability (application of the Hoppin house style), accessibility (for all users irrespective of disability, age, etc.), minimum equipment (e.g. bicycle sheds, information supports, etc.), alignment with mobility plans, etc."
      },
      "equivalent": [],
      "parents": [
        "https://purl.eu/ns/mobility/passenger-transport-hubs#RegisteredTransportHub"
      ]
    },
    {
      "uri": "https://purl.eu/ns/mobility/passenger-transport-hubs#InformationCarrier",
      "name": "InformationCarrier",
      "label": {
        "en": "InformationCarrier"
      },
      "sort": "InformationCarrier",
      "description": {
        "en": "Panel or board or similar infrastructure on which information is displayed."
      },
      "usage": {
        "en": "Intended as a means of communication, usually used in public spaces to inform passers-by. Both static carriers such as information boards with a city map and dynamic systems with e.g. public transport departure times are possible."
      },
      "equivalent": [],
      "parents": [
        "https://data.vlaanderen.be/ns/openbaardomein/infrastructuur#Infrastructuurelement"
      ]
    },
    {
      "uri": "https://purl.eu/ns/mobility/passenger-transport-hubs#RegisteredTransportHub",
      "name": "RegisteredPassengerTransportHub",
      "label": {
        "en": "RegisteredPassengerTransportHub"
      },
      "sort": "RegisteredPassengerTransportHub",
      "description": {
        "en": "Passenger Transport Hub recognised as such."
      },
      "usage": {
        "en": "This recognition implies that a Transport Node is formally designated as a Transport Node. Typically, this is done by the government to give the node an official character (recognisable for the user, meets imposed quality requirements...)."
      },
      "equivalent": [],
      "parents": [
        "https://purl.eu/ns/mobility/passenger-transport-hubs#TransportHub"
      ]
    },
    {
      "uri": "https://purl.eu/ns/mobility/passenger-transport-hubs#ResourceReport",
      "name": "ResourceReport",
      "label": {
        "en": "ResourceReport"
      },
      "sort": "ResourceReport",
      "description": {
        "en": "Report of how many Resources with similar characteristics of availability, type, service, location etc are present at a certain point in time."
      },
      "usage": {
        "en": "E.g. The number of available bikes at the trainstation."
      },
