    {
      "uri": "https://purl.eu/ns/mobility/passenger-transport-hubs#HoppinColumn",
      "name": "HoppinColumn",
      "label": {
        "en": "HoppinColumn"
      },
      "sort": "HoppinColumn",
      "description": {
        "en": "Information column of a Hoppinpoint."
      },
      "usage": {
        "en": "Intended to indicate that a Transport hub is a Hoppin point and/or to inform about the transport possibilities offered there.\nMore info can be found in the implementation model: https://wegenenverkeer.data.vlaanderen.be/doc/implementatiemodel/hoppinpunten/"
      },
      "equivalent": [],
